{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 1,       // Means warning
    "prettier/prettier": 1, // Means error
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["warn",
    {
      "argsIgnorePattern": "^_"
    }],
    "@typescript-eslint/no-explicit-any": ["off"]
  }
}
